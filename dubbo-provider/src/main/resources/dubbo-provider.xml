<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">


      <dubbo:application name="provider-redis"/>

      <dubbo:registry protocol="zookeeper" address="192.168.86.90:2181"/>
       <dubbo:protocol name="dubbo" port="20885" />
      <dubbo:service interface="com.example.dubbointerface.interfaces.IRedis" ref="iRedisService" loadbalance="roundrobin" timeout="5000"/>
</beans>